#!/bin/bash
sed -i "s+FREETZ_PACKAGE_ACME=y+# FREETZ_PACKAGE_ACME is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_PACKAGE_CA_BUNDLE=y+# FREETZ_PACKAGE_CA_BUNDLE is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_PACKAGE_OPENSSL=y+# FREETZ_PACKAGE_OPENSSL is not set+g" $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_OPENSSL_TRACE $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_OPENSSL_STATIC $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libcrypto_WITH_EC=y+# FREETZ_LIB_libcrypto_WITH_EC is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_BUSYBOX___V127_DESKTOP=y+# FREETZ_BUSYBOX___V127_DESKTOP is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_BUSYBOX___V134_DESKTOP=y+# FREETZ_BUSYBOX___V134_DESKTOP is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_BUSYBOX___V135_DESKTOP=y+# FREETZ_BUSYBOX___V135_DESKTOP is not set+g" $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_ACME_deploy $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_ACME_dnsapi $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_ACME_notify $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_ACME_SOCAT $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_PACKAGE_WGET=y+# FREETZ_PACKAGE_WGET is not set+g" $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_WGET_WITH_SSL $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_WGET_OPENSSL $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_WGET_GNUTLS $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_WGET_CA_BUNDLE $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_WGET_STATIC $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_BUSYBOX_DESKTOP=y $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
