#!/bin/bash
sed -i "s+FREETZ_PACKAGE_CALLMONITOR=y+# FREETZ_PACKAGE_CALLMONITOR is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_PACKAGE_PHONEBOOK_TOOLS=y+# FREETZ_PACKAGE_PHONEBOOK_TOOLS is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_BUSYBOX___V127_UUENCODE=y+# FREETZ_BUSYBOX___V127_UUENCODE is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_BUSYBOX___V127_NC_SERVER=y+# FREETZ_BUSYBOX___V127_NC_SERVER is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_BUSYBOX___V134_UUENCODE=y+# FREETZ_BUSYBOX___V134_UUENCODE is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_BUSYBOX___V134_NC_SERVER=y+# FREETZ_BUSYBOX___V134_NC_SERVER is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_BUSYBOX___V135_UUENCODE=y+# FREETZ_BUSYBOX___V135_UUENCODE is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_BUSYBOX___V135_NC_SERVER=y+# FREETZ_BUSYBOX___V135_NC_SERVER is not set+g" $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_BUSYBOX_NC_SERVER $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_BUSYBOX_UUENCODE $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_CALLMONITOR_webif $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_CALLMONITOR_actions $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_CALLMONITOR_monitor $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_CALLMONITOR_phonebook $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
