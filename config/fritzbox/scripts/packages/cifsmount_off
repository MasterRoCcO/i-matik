#!/bin/bash
sed -i "s+FREETZ_PACKAGE_CIFSMOUNT=y+# FREETZ_PACKAGE_CIFSMOUNT is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_MODULE_arc4=y+# FREETZ_MODULE_arc4 is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_MODULE_cifs=y+# FREETZ_MODULE_cifs is not set+g" $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_MODULE_cmac $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_MODULE_ecb $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_MODULE_md4 $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_CIFSMOUNT_REMOVE_WEBIF $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei
mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
