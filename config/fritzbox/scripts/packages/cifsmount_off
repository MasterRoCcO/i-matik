#!/bin/bash
sed -i "s+FREETZ_PACKAGE_CIFSMOUNT=y+# FREETZ_PACKAGE_CIFSMOUNT is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_MODULE_arc4=y+# FREETZ_MODULE_arc4 is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_MODULE_cifs=y+# FREETZ_MODULE_cifs is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_REPLACE_KERNEL=y+# FREETZ_REPLACE_KERNEL is not set+g" $FREETZ_SSL_HOME_PATH/.config 
grep -v FREETZ_MODULE_cmac $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config 
grep -v FREETZ_MODULE_ecb $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config 
grep -v FREETZ_MODULE_md4 $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config 
grep -v FREETZ_PACKAGE_CIFSMOUNT_REMOVE_WEBIF $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config 
grep -v FREETZ_MODULE_des_generic $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config 
grep -v FREETZ_MODULE_hmac $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config 
grep -v FREETZ_MODULE_md5 $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v CONFIG_CIFS_UPCALL $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}  > $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei && mv $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}
grep -v CONFIG_CIFS_DFS_UPCALL $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}  > $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei && mv $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}
grep -v CONFIG_NFSD $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}  > $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei && mv $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}
grep -v CONFIG_NFSD_V3 $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}  > $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei && mv $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}
grep -v CONFIG_NFSD_V3_ACL $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}  > $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei && mv $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}
grep -v CONFIG_NFSD_V4 $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}  > $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei && mv $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}
grep -v CONFIG_CUSE $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}  > $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei && mv $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}
grep -v CONFIG_NFSD $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}  > $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei && mv $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}
grep -v CONFIG_NFSD_V3 $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}  > $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei && mv $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}
grep -v CONFIG_NFSD_V3_ACL $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}  > $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei && mv $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}
grep -v CONFIG_NFSD_V4 $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}  > $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei && mv $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}
grep -v CONFIG_NFSD_DEPRECATED $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}  > $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei && mv $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}
