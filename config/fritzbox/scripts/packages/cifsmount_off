grep -v CONFIG_CIFS_WEAK_PW_HASH $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}  > $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei && mv $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}
