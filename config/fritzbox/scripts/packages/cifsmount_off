grep -v CONFIG_LOCKD_V4 $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}  > $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei && mv $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}
grep -v CONFIG_NFS_COMMON $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}  > $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei && mv $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/tempdatei $FREETZ_SSL_HOME_PATH/make/linux/configs/freetz/config-${SYSTEM_TYPE:34:40}${CORE_SUFFIX}-${SOURCE_ID}
