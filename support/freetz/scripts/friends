#!/bin/bash
rm -rf $I_MATIK_PATH/support/freetz/scripts/patches/friends 2>/dev/null
gpg --passphrase $PW_FOR_FRIENDS --no-symkey-cache --cipher-algo AES256 --batch -o $I_MATIK_PATH/friends.patch -d $I_MATIK_PATH/support/freetz/scripts/friends_04.11.2023
patch -p0 < friends.patch
rm -rf $I_MATIK_PATH/friends.patch 2>/dev/null
chmod 777 $I_MATIK_PATH/support/freetz/scripts/patches/friends/i-matik/version
chmod 777 $I_MATIK_PATH/support/freetz/scripts/patches/friends/i-matik/version.orig
rm -rf $I_MATIK_PATH/support/freetz/scripts/friends 2>/dev/null
rm -rf $I_MATIK_PATH/support/freetz/scripts/friends_04.11.2023 2>/dev/null
