	./i-matik 7530AX.11 -c -nc -s
#ohne oscam -c -s
	./i-matik 3000.11 -c -nc -s
# no inmemory ohne oscam
	./i-matik 1200AX.11 2400.11 3000AX.11 5530.11 6000.11 -c -nc -s
