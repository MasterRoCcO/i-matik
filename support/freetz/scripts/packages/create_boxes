	mv $I_MATIK_PATH/support/freetz/openssl_10_os72 $I_MATIK_PATH/support/freetz/openssl_10
	mv $I_MATIK_PATH/support/freetz/openssl_11 $I_MATIK_PATH/support/freetz/openssl_11_os74
