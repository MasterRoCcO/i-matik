#!/bin/bash
sed -i "s+# FREETZ_PACKAGE_TOR is not set+FREETZ_PACKAGE_TOR=y+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+# FREETZ_PACKAGE_TOR_GEOIP_V4 is not set+FREETZ_PACKAGE_TOR_GEOIP_V4y+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+# FREETZ_PACKAGE_TOR_GEOIP_V6 is not set+FREETZ_PACKAGE_TOR_GEOIP_V6y+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+# FREETZ_PACKAGE_TOR_STATIC is not set+FREETZ_PACKAGE_TOR_STATICy+g" $FREETZ_SSL_HOME_PATH/.config
