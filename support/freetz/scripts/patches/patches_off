mv $FREETZ_SSL_HOME_PATH/make/dbus/external.files.orig $FREETZ_SSL_HOME_PATH/make/dbus/external.files 2>/dev/null
mv $FREETZ_SSL_HOME_PATH/make/dbus/external.in.libs.orig $FREETZ_SSL_HOME_PATH/make/dbus/external.in.libs 2>/dev/null
