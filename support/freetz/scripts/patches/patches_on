#!/bin/bash
cp -ar $I_MATIK_PATH/support/freetz/patches/busybox_1_27.patch "$FREETZ_SSL_HOME_PATH/" 2>/dev/null
cp -ar $I_MATIK_PATH/support/freetz/patches/login.patch "$FREETZ_SSL_HOME_PATH/" 2>/dev/null
cp -ar $I_MATIK_PATH/support/freetz/patches/menu-new.patch "$FREETZ_SSL_HOME_PATH/" 2>/dev/null
cp -ar $I_MATIK_PATH/support/freetz/patches/yf-bootmanager.patch "$FREETZ_SSL_HOME_PATH/" 2>/dev/null
cd $FREETZ_SSL_HOME_PATH
patch -p0 -b < busybox_1_27.patch
patch -p0 -b < login.patch
patch -p0 -b < menu-new.patch
patch -p0 -b < yf-bootmanager.patch
rm -rf "$FREETZ_SSL_HOME_PATH/busybox_1_27.patch" 2>/dev/null
rm -rf "$FREETZ_SSL_HOME_PATH/login.patch" 2>/dev/null
rm -rf "$FREETZ_SSL_HOME_PATH/menu-new.patch" 2>/dev/null
rm -rf "$FREETZ_SSL_HOME_PATH/yf-bootmanager.patch" 2>/dev/null
