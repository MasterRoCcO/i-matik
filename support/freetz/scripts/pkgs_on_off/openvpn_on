#!/bin/bash

ed $FREETZ_SSL_HOME_PATH/.config <<EOF
/# FREETZ_PACKAGE_OPENVPN is not set/a
# FREETZ_PACKAGE_OPENVPN_USE_V0_CGI is not set
FREETZ_PACKAGE_OPENVPN_USE_V1_CGI=y
# FREETZ_PACKAGE_OPENVPN_USE_V2_CGI is not set
FREETZ_PACKAGE_OPENVPN_OPENSSL=y
# FREETZ_PACKAGE_OPENVPN_MBEDTLS is not set
FREETZ_PACKAGE_OPENVPN_WITH_BLOWFISH=y
FREETZ_PACKAGE_OPENVPN_WITH_LZO=y
# FREETZ_PACKAGE_OPENVPN_WITH_LZ4 is not set
FREETZ_PACKAGE_OPENVPN_WITH_TRAFFIC_OBFUSCATION=y
FREETZ_PACKAGE_OPENVPN_WITH_MGMNT=y
FREETZ_PACKAGE_OPENVPN_ENABLE_SMALL=y
FREETZ_PACKAGE_OPENVPN_USE_IPROUTE=y
.
wq
EOF
sed -i "s+# FREETZ_PACKAGE_OPENVPN is not set+FREETZ_PACKAGE_OPENVPN=y+g" $FREETZ_SSL_HOME_PATH/.config
#sed -i "s+FREETZ_USER_LEVEL_EXPERT=y+# FREETZ_USER_LEVEL_EXPERT is not set+g" $FREETZ_SSL_HOME_PATH/.config
#sed -i "s+# FREETZ_USER_LEVEL_DEVELOPER is not set+FREETZ_USER_LEVEL_DEVELOPER=y+g" $FREETZ_SSL_HOME_PATH/.config
#sed -i '$a# FREETZ_PACKAGE_IPTABLES is not set' $FREETZ_SSL_HOME_PATH/.config
#sed -i "s+# FREETZ_PACKAGE_IPTABLES is not set+FREETZ_PACKAGE_IPTABLES=y+g" $FREETZ_SSL_HOME_PATH/.config
