#!/bin/bash
sed -i "s+FREETZ_PACKAGE_RRDSTATS=y+# FREETZ_PACKAGE_RRDSTATS is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_PACKAGE_RRDTOOL=y+# FREETZ_PACKAGE_RRDTOOL is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libz=y+# FREETZ_LIB_libz is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libfreetype=y+# FREETZ_LIB_libfreetype is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libart_lgpl_2=y+# FREETZ_LIB_libart_lgpl_2 is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libpng12=y+# FREETZ_LIB_libpng12 is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_librrd=y+# FREETZ_LIB_librrd is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libglib_2=y+# FREETZ_LIB_libglib_2 is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_BUSYBOX___V127_START_STOP_DAEMON=y+# FREETZ_BUSYBOX___V127_START_STOP_DAEMON is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_BUSYBOX___V136_START_STOP_DAEMON=y+# FREETZ_BUSYBOX___V136_START_STOP_DAEMON is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_REMOVE_AVMCOUNTERD=y+# FREETZ_REMOVE_AVMCOUNTERD is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_PACKAGE_DEJAVU_FONTS_TTF=y+# FREETZ_PACKAGE_DEJAVU_FONTS_TTF is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_PACKAGE_DEJAVU_FONTS_TTF_DejaVuSansMono=y+# FREETZ_PACKAGE_DEJAVU_FONTS_TTF_DejaVuSansMono is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_PACKAGE_FONTCONFIG=y+# FREETZ_PACKAGE_FONTCONFIG is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libfribidi=y+# FREETZ_LIB_libfribidi is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libgio_2=y+# FREETZ_LIB_libgio_2 is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libgobject_2=y+# FREETZ_LIB_libgobject_2 is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libgmodule_2=y+# FREETZ_LIB_libgmodule_2 is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libcairo=y+# FREETZ_LIB_libcairo is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libfontconfig=y+# FREETZ_LIB_libfontconfig is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libharfbuzz=y+# FREETZ_LIB_libharfbuzz is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libpango_1=y+# FREETZ_LIB_libpango_1 is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libpangoft2_1=y+# FREETZ_LIB_libpangoft2_1 is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libpangocairo_1=y+# FREETZ_LIB_libpangocairo_1 is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libpixman_1=y+# FREETZ_LIB_libpixman_1 is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libxml2=y+# FREETZ_LIB_libxml2 is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libffi=y+# FREETZ_LIB_libffi is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_STDCXXLIB_FORCE_GNULIBSTDCXX=y+# FREETZ_STDCXXLIB_FORCE_GNULIBSTDCXX is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_STDCXXLIB_USE_GNULIBSTDCXX=y+# FREETZ_STDCXXLIB_USE_GNULIBSTDCXX is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libpng16=y+# FREETZ_LIB_libpng16 is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libpcre=y+# FREETZ_LIB_libpcre is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libz=y+# FREETZ_LIB_libz is not set+g" $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_RRDSTATS_CABLEMODEM $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v PACKAGE_RRDSTATS_SEGMENTLOAD $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_RRDSTATS_STORAGE $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_RRDSTATS_NETWORK $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_RRDSTATS_DIGITEMP $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_RRDSTATS_SMARTHOME $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_BUSYBOX_FEATURE_START_STOP_DAEMON_FANCY $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_BUSYBOX___V127_FEATURE_START_STOP_DAEMON_FANCY $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_BUSYBOX___V136_FEATURE_START_STOP_DAEMON_FANCY $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_BUSYBOX___V127_FEATURE_START_STOP_DAEMON_LONG_OPTIONS $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_BUSYBOX___V136_FEATURE_START_STOP_DAEMON_LONG_OPTIONS $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_RRDSTATS_TEMPERATURE_SENSOR $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_BUSYBOX_START_STOP_DAEMON $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_PACKAGE_CA_BUNDLE=y+# FREETZ_PACKAGE_CA_BUNDLE is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_PACKAGE_DVBTUNE=y+# FREETZ_PACKAGE_DVBTUNE is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_PACKAGE_WGET=y+# FREETZ_PACKAGE_WGET is not set+g" $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_LIB_libcrypto_WITH_EC=y+# FREETZ_LIB_libcrypto_WITH_EC is not set+g" $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_DVBSNOOP $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_WGET_WITH_SSL $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_WGET_OPENSSL $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_WGET_GNUTLS $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_WGET_CA_BUNDLE $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_WGET_STATIC $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
sed -i "s+FREETZ_PACKAGE_DIGITEMP=y+# FREETZ_PACKAGE_DIGITEMP is not set+g" $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_DIGITEMP_DS2490 $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_DIGITEMP_DS9097U $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
grep -v FREETZ_PACKAGE_DIGITEMP_DS9097 $FREETZ_SSL_HOME_PATH/.config  > $FREETZ_SSL_HOME_PATH/tempdatei && mv $FREETZ_SSL_HOME_PATH/tempdatei $FREETZ_SSL_HOME_PATH/.config
sed -i "s+rrdtool_1_2_${FUD_COMMENT_RRD}+${FUD_COMMENT_RRD}+g" $FREETZ_SSL_HOME_PATH/.config 2>/dev/null
sed -i "s+rrdtool_1_8_${FUD_COMMENT_RRD}+${FUD_COMMENT_RRD}+g" $FREETZ_SSL_HOME_PATH/.config 2>/dev/null
